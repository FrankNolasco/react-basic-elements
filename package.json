{
  "name": "fnv-core-elements",
  "version": "0.0.1",
  "type": "module",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "main": "dist/index.js",
  "license": "MIT",
  "homepage": "https://franknolasco.github.io/Portafolio/",
  "repository": {
    "url": "https://github.com/FrankNolasco/react-basic-elements",
    "type": "git"
  },
  "release": {
    "branches": [
      "production"
    ]
  },
  "exports": {
    ".": {
      "require": "./dist/react-basic-elements.cjs",
      "import": "./dist/react-basic-elements.es.js"
    },
    "./styles.css": {
      "require": "./dist/styles.css",
      "default": "./dist/styles.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "clean": "rimraf dist/",
    "my-publish": "npm version patch && npm publish"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-plugin-dts": "^3.3.1",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.2.0-alpha.0",
    "@storybook/addon-interactions": "^7.2.0-alpha.0",
    "@storybook/addon-links": "^7.2.0-alpha.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.2.0-alpha.0",
    "@storybook/react": "^7.2.0-alpha.0",
    "@storybook/react-vite": "^7.2.0-alpha.0",
    "@storybook/testing-library": "^0.2.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "copyfiles": "^2.4.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.13",
    "rimraf": "^5.0.5",
    "storybook": "^7.2.0-alpha.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "plugins": [
    "@semantic-realease/commit-analyzer",
    "@semantic-realease/release-notes-generator",
    "@semantic-realease/changelog",
    "@semantic-realease/github",
    "@semantic-realease/npm",
    "@semantic-realease/git"
  ]
}
